{
    "name": "epsi-course-agenda",
    "version": "0.1.0",
    "description": "API to maintain Google Agenda",
    "main": "src/index.js",
    "scripts": {
        "build": "babel src -d dist --copy-files",
        "dev": "nodemon --exec npx babel-node -r dotenv/config src/index.js",
        "prestart": "npm run build",
        "start": "node dist/index.js",
        "test": "jest",
        "validate:lint": "eslint src",
        "validate:prettier": "prettier --check ./src/**/*.js",
        "validate": "npm run validate:lint && npm run validate:prettier"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "pg": "^8.7.1",
        "sequelize": "^6.9.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.8",
        "@babel/node": "^7.15.8",
        "@babel/plugin-proposal-throw-expressions": "^7.14.5",
        "@babel/preset-env": "^7.15.8",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-node": "^4.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.2.5",
        "nodemon": "^2.0.13",
        "prettier": "^2.4.1"
    },
    "keywords": [],
    "author": "fabien-renaud",
    "license": "ISC"
}
